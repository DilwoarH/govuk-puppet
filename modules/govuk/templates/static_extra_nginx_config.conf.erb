location = /static/a {
  expires -1;
  add_header Last-Modified "";

  default_type text/plain;
  return 200 '';
}

location /__canary__ {
  default_type application/json;
  add_header cache-control "max-age=0, no-store, no-cache";
  return 200 '{"message": "Tweet tweet"}\n';
}

location /robots.txt {
  expires 1w;
}
