# Configuration for collectd-rabbitmq plugin

TypesDB "/usr/share/collectd/types.db" "/usr/share/collectd/types.db.rabbitmq"

LoadPlugin python
<Plugin python>
  LogTraces true
  Interactive false
  Import "collectd_rabbitmq.collectd_plugin"
  <Module "collectd_rabbitmq.collectd_plugin">
    Username "monitoring"
    Password "<%= @monitoring_password %>"
    Realm "RabbitMQ Management"
    Host "localhost"
    Port "15672"
  </Module>
</Plugin>
