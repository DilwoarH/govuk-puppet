server {
  server_name  preview-ertp-api.alphagov.co.uk;
  listen *:80;


  location / {
    proxy_pass http://127.0.0.1:9900/;
    deny all;
    auth_basic            "Enter your ertp api preview password";
    auth_basic_user_file  /etc/nginx/htpasswd/htpasswd.ertp.preview.api;
    satisfy any;
  }
}
