#!/bin/bash
<%
# Sort ensures the file from changing on every run for Rubies which don't
# have ordered Hash.
@smokey_vars.sort_by { |k,v| k }.each do |key, val|
  next if val == 'UNSET'
-%>
export <%= key -%>="<%= val -%>"
<% end -%>
<% if @spoof_target_domain -%>
export SPOOF_TARGET_DOMAIN=<%= @spoof_target_domain %>
<% end -%>
