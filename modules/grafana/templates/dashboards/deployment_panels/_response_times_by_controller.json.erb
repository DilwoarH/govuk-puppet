  "pointradius": 5,
  "points": false,
  "renderer": "flot",
  "seriesOverrides": [],
  "span": 12,
  "stack": false,
  "steppedLine": false,
  "targets": [
    {
      "bucketAggs": [
        {
          "fake": true,
          "field": "@fields.controller",
          "id": "3",
          "settings": {
            "order": "desc",
            "orderBy": "_count",
            "size": "10"
          },
          "type": "terms"
        },
        {
          "field": "@timestamp",
          "id": "2",
          "settings": {
            "interval": "15s",
            "min_doc_count": 0,
            "trimEdges": 0
          },
          "type": "date_histogram"
        }
      ],
      "dsType": "elasticsearch",
      "metrics": [
        {
          "field": "@fields.duration",
          "id": "1",
          "meta": {},
          "settings": {
            "percents": [
              "90"
            ]
          },
          "type": "percentiles"
        }
      ],
      <% if @logit_only -%>
      "query": "application:<%= @app_name %>",
      <% else -%>
      "query": "@fields.application:<%= @app_name %>",
      <% end -%>
      "refId": "A",
      "timeField": "@timestamp"
    }
  ],
  "timeFrom": "6h",
  "timeShift": null,
  "title": "90th Percentile Response Time by Controller",
  "tooltip": {
    "msResolution": true,
    "shared": true,
    "sort": 2,
    "value_type": "cumulative"
